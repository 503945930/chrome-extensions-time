{
    "manifest_version": 2,
    "name": "time toole",
    "description": "display current time",
    "version": "1.0",
    "chrome_url_overrides": {
        "newtab": "main.html"
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "./img/icon.jpg",
        "default_popup": "popup.html",
        "default_title": "time tool"
    }
}